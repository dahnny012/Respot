div(class="modal fade", id="quizlet-deck-modal", tabindex="-1", role="dialog", style="display: none;")
    div.modal-dialog
        div.formmodal-container.big
            h1 Search Quizlet
            form(id="quizlet-search")
                input(type="text",name='q', placeholder="Search term" required)
                input(type="hidden" name="page" value=1)
                input(type='submit', value="Search" class="form formmodal-submit")
                nav
             ul.pager
                li
                    a(href='#') Previous
                li
                    a(href='#') Next
            .list-group(id="search-results")

                


                
